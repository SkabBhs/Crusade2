                <characteristic name="Button Effect" typeId="dcd0-b896-8ccf-ea83">[LETHAL HITS] ability</characteristic>
              </characteristics>
            </profile>
            <profile id="3ccb-2766-28c3-4c35" name="1-2" hidden="false" typeId="2ece-65f0-756f-bc77" typeName="Try Dat Button! - D6">
              <characteristics>
                <characteristic name="Button Effect" typeId="dcd0-b896-8ccf-ea83">[SUSTAINED HITS 1] ability</characteristic>
              </characteristics>
